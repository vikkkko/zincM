//!     "output": "30"
//! } ] }

const ARRAY_SIZE: u8 = 10;

fn main(array: [u8; ARRAY_SIZE]) -> u8 {
    let mut sum = 0;
    for i in 7..=3 {
        sum += array[i];
    }
    sum
}
