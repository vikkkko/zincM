//!
//! The 'inner' contract entry.
//!

contract Inner {
    pub x: u8;
    pub y: u8;
    pub z: u8;

    pub fn new(x: u8, y: u8, z: u8) -> Self {
        Self {
            x: x,
            y: y,
            z: z,
        }
    }

    pub fn sum(self) -> u8 {
        self.x + self.y + self.z
    }
}
