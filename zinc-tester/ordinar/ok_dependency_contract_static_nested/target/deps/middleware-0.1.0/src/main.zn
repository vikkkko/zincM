//!
//! The 'middleware' contract entry.
//!

contract Middleware {
    pub fn add(a: u8, b: u8) -> u8 {
        math::Math::add(a, b)
    }

    pub fn sub(a: u8, b: u8) -> u8 {
        math::Math::sub(a, b)
    }

    pub fn mul(a: u8, b: u8) -> u8 {
        math::Math::mul(a, b)
    }

    pub fn div(a: u8, b: u8) -> u8 {
        math::Math::div(a, b)
    }

    pub fn rem(a: u8, b: u8) -> u8 {
        math::Math::rem(a, b)
    }
}
