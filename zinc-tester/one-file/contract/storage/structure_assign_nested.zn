//!     "case": "default",
//!     "method": "main",
//!     "input": {
//!         "arg": "42"
//!     },
//!     "output": {
//!         "result": "126",
//!         "root_hash": "0x0"
//!     }
//! } ] }

struct Data {
    inner: Inner,
}

struct Inner {
    value: u8,
}

contract Test {
    data: Data;

    const TEST: u8 = 42;

    pub fn main(mut self, arg: u8) -> u8 {
        self.data.inner.value = arg;

        arg + Self::TEST + self.data.inner.value
    }
}
